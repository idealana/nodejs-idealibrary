generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id            Int          @id @default(autoincrement())
  title         String
  description   String?
  author        String
  createdBy     Int
  createdAt     DateTime
  updatedBy     Int?
  updatedAt     DateTime?

  @@map("book")
}
